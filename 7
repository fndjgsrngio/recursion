package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(rev(n));
    }
    public static void rev(int n){
    if(n==0)
        return;
    int numbers = sc.nextInt();
    rev(n - 1);
    System.out.print(numbers + " ");
    }
}
